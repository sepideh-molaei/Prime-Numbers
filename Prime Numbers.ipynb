    "remain = []\n",
    "def prime (num):\n",
    "  for i in range(2,num):\n",
    "    remain.append(num % i) \n",
    "  if 0 in remain:\n",
    "     print(\"Not Prime\")\n",
    "  else:\n",
    "     print(\"Prime\")  \n",
    "\n",
    "num = int (input(\"Please add a number: \"))\n",
    "prime(num)"
   ]
  }
